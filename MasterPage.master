</html>


<script>
    $(document).ready(function () {
        // Apply Select2 to the dropdown
        $("#ddlChemists").select2({
            placeholder: "-- Select Drug --",
            allowClear: true // Allows clearing the selection
        });
    });
</script>

<script>
    function resetForm() {
        const form = document.getElementById('<%= mainForm.ClientID %>');
        form.reset();
    }
</script>
